local ksp=game:GetService'KeyframeSequenceProvider'
local extraAnims={}
local valid=true

function charAnimExists(char)
	return not not extraAnims[char]
end

function firstCase(s)
	return s:sub(1,1):upper()..s:sub(2):lower()
end

function createAnimTrack(char,a,params)
	local anim=char.Humanoid.Animator:LoadAnimation(a)
	
	--Fills in the parameters.
	for i,p in pairs(params or{}) do
		if i:lower()~='speed' and not pcall(function()anim[i]=p end) then
			error('\''..i..'\' is not a valid property of \'AnimationTrack\'',5)
			return
		end
	end
	return anim
end

function createAnim(content)
	local a=Instance.new'Animation'
	a.AnimationId=content
	return a
end

function playAnimContent(char,c,...)
	playAnimation(char,createAnim(c),...)
end

function playAnimation(char,a,params)
	local tracks=char.Humanoid:GetPlayingAnimationTracks()
	for i,t in pairs(tracks) do
		t:Stop()
		t:destroy()
	end
	
	if charAnimExists(char) then
		local anim=extraAnims[char]
		anim:Stop()
		anim:destroy()
	end
	
	char.Animate.Disabled=true
	local anim=createAnimTrack(char,a,params)
	extraAnims[char]=anim
	anim:Play(nil,nil,params.Speed)
	if not anim.Looped then
		--Keeping to the API's asynchronous theme!
		spawn(function()
			anim.Stopped:wait()
			stopCustomAnims(char)
		end)
	end
end

function playAnimationName(char,name,...)
	local v=char.Animate:findFirstChild(name)
	if not v then
		error('No animation with the name \''..name'\' exists.',3)
		return
	end
	playAnimation(char,v:FindFirstChildWhichIsA'Animation',...)
end

function playAnimationID(char,id,...)
	return playAnimContent(char,'http://www.roblox.com/asset/?id='..id,...)
end

function playAnimationSeq(char,seq,...)
	if game:service'RunService':IsStudio() then
		return playAnimContent(char,ksp:RegisterActiveKeyframeSequence(seq),...)
	else
		error('Animations can only be played in Studio and when the game is running.',3)
		return
	end
end

function stopCustomAnims(char)
	char.Animate.Disabled=false
end

function getSeqFromChar(char)
	for seq,t in pairs(extraAnims) do
		if t.char==char then
			return seq
		end
	end
end

return{function(options)
	
	--Character
	local char=options.Character
	
	--Animation Sequence
	local seq=options.AnimSequence
	
	--Long
	local id=options.AnimID
	
	--String
	local name=options.AnimName
	
	--Property table
	local params=options.AnimParams
	
	do local e
		if not _G.AnimationAPI.IsID(id) then e=true
			error('\'AnimID\' should be a positive integer.',2)
		elseif not _G.AnimationAPI.IsCharacter(char)then e=true
			error('\'Character\' is not a valid value.',2)
		elseif not name then
			if seq and not seq:isA'KeyframeSequence' then e=true
				error('\'AnimSequence\' is not a valid value.',2)
			elseif id and typeof(id)~='number' then e=true
				error('\'AnimID\' is not a valid value.',2)
			end
		end
		
		if seq and id then e=true
			if name then error('\'AnimName\', \'AnimSequence\', and \'AnimID\' were all passed in; please only use one of them.',2)
			else error'Both \'AnimSequence\' and \'AnimID\' were passed in; please get rid of either one.'end
		elseif not seq and not id then e=true
			error('Neither \'AnimName\', \'AnimSequence\', nor \'AnimID\' were passed in; please include any one of them.',2)
		end
		
		if e then
		return end
	end
	
	if name then
		playAnimationName(char,name,params)
	elseif seq then
		playAnimationSeq(char,seq,params)
	elseif id then
		playAnimationID(char,id,params)
	else
		stopCustomAnims(char)
	end
end,stopCustomAnims}]]></ProtectedString>
