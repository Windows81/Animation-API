				<ProtectedString name="Source"><![CDATA[--// 1630228 • 1: 2018-05-28/2018-06? • 2: 2019-01-06/2019-02-07 • +1 714 463 5142 //--
local api

--Pulls up the utility functions.
util=require(script.Util)

--Gets the character from that annoying metaobject (or itself).
function getChar(obj)
	
	--Returns the character if it's not itself?
	if api.IsCharacter(obj) then
		return obj
	
	--It's a metaobject from a table.
	elseif type(obj)=='table' then
		
		--Metatable property that identifies it's a metaobject!
		local qush=getmetatable(obj).chiqish=='charMetaclass'
		if qush then return obj.Character end
	end
end

--Base values for structs for efficiency...
local baseCF=CFrame.new()

--Implicit casting.
function cast(ob,to)
	local obT=typeof(ob)
	local toT=typeof(to)
	if obT==toT then
		return ob
	elseif obT=='CFrame' and toT=='Vector3' then
		return ob.p
	elseif obT=='Vector3' and toT=='CFrame' then
		return CFrame.new(ob)
	else return nil
	end
end

--Keeps a local connection to the clients.
local fireClients
local lcl=script.AnimLocal
local isCl=game['Run Service']:IsClient()

--Some sloppy workaround for JJSploit support.
if 'script'=='sc' then connect=nil
	local src=script.AnimLocal.Source
		:match'%-%-{B}%-%-(.+)%-%-{F}%-%-'
		:gsub('require','newReq')
	loadstring(src)
	fireClients=connect
	
elseif isCl then
	local evt=script.BindableEvent
	lcl.Event.Value=evt
	
	--A boring wrapper method.
	function fireClients(...)evt:Fire(...)end
else
	--Ensures that the client may be able to access the scripts' contents.
	script.Parent=game.Workspace
	
	local evt=script.RemoteEvent
	lcl.Event.Value=evt
	
	--Insert a copy of 'AnimLocal' where not found.
	function insertLcl(par)
		if not par:findFirstChild'AnimLocal' then
			lcl:clone().Parent=par
		end
	end
	
	--Does the inserting.
	lcl.Parent=game.StarterPlayer.StarterPlayerScripts
	for i,pl in pairs(game.Players:children()) do
		insertLcl(pl:WaitForChild'PlayerScripts')
	end

	--Just another boring wrapper method.
	function fireClients(...) evt:FireAllClients(...)end
end

--API METHODS
api={
	
	--Evaluator methods; intende to be used mainly for debugging purposes.
	IsID=function(n)return typeof(n)=='number'and n%1==0 and n>0 end,
	IsAppearance=function(app)
		return typeof(app)=='Instance'and app:findFirstChild'IsR15'
	end,
	IsCharacter=function(char)
		return typeof(char)=='Instance'and char:FindFirstChildOfClass'Humanoid'
	end,
	IsCharMetaobject=function(char)
		if typeof(char)=='table' then
			local t=getmetatable(char)
			return t and t.chiqish=='charMetaclass'
		end
		return false
	end,
	
	--Retrieves model containing the assets which the player is wearing.
	GetAppearance=function(options)
		
		--Long
		local id=options.UserID
			
		do local e
			if typeof(id)~='number'and id%1==0 then e=true
				error('\'UserID\' must be a positive integer.',2)
			end
			
			if e then
			return end
		end
		
		local m=game.Players:GetCharacterAppearanceAsync(id)
		--[[
		local folders={}
		for i,f in pairs(m:children()) do
			if f:isA'Folder' then
				local F=folders[f.Name]or Instance.new'Folder'
				folders[f.Name],F.name=F,f.Name
				for i,o in pairs(f:children()) do
					o.Parent=F
				end f:destroy()
			end
		end
		for i,f in pairs(folders) do
			f.Parent=m end
		]]
		return m
	end,
	
	--'Dresses up' an empty rig with a specified user's appearance.
	DressUpRig=function(options)
		
