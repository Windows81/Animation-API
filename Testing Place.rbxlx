					<ProtectedString name="Source"><![CDATA[--Table of hidden players.
local hidden={}

--Whether new players are hidden.
local default=false

local function showChar(ch)
	for i,p in pairs(ch:GetDescendants()) do
		if p:isA'BasePart' then
			p.Transparency=p.Transparency-1
		end
	end
end

local function hideChar(ch)
	for i,p in pairs(ch:GetDescendants()) do
		if p:isA'BasePart' then
			p.Transparency=p.Transparency+1
		end
	end
end

--Determines whether to hide or do nothing.
local function iter8Ch(pl,ch)
	local h=hidden[pl]
	
	--Only hides, since it's just got spawned.
	if h then hideChar(ch)
	
	--First-time setup; the table value gets set.
	elseif h==nil then hidden[pl]=default
		if default then hideChar(ch)end
	end
end

local function iter8Pl(pl)
	--Connects the function to your characters.
	if pl.Character then iter8Ch(pl,pl.Character)end
	pl.CharacterAdded:Connect(function(ch)iter8Ch(pl,ch)end)
end

--Connects the function to your players.
for i,pl in pairs(game.Players:children()) do iter8Pl(pl)end
game.Players.PlayerAdded:Connect(iter8Pl)

return{
	setChar=function(pl,b)
		
		--Does nothing if there's nothing to change.
		local h=hidden[pl]
		if h==b then return end
		
		--Does the hiding process and all.
		hidden[pl]=b
		local ch=pl.Character
		if b then hideChar(ch)
		else showChar(ch)end
	end,
	setDef=function(b)
		default=b
	end,
}]]></ProtectedString>
