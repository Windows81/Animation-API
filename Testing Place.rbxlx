					<ProtectedString name="Source"><![CDATA[local function GetEnumValue(enum,eType)
	
	--Checks if 'eType' is a valid Enum type-string.
	local vEnumStr=type(eType)=='string'and
		pcall(function()return Enum[eType]end)
	
	--Either converts the 'eType' to its Enum object or errors out.
	if vEnumStr then eType=Enum[eType]
	elseif typeof(eType)~='Enum' then
		--error('Internal: \'eType\' is not valid.',2)
		return
	end
	
	--This is where we assume 'eType' is an Enum.
	
	--Checks if the EnumItem was already passed into and returns conditionally.
	if typeof(enum)=='EnumItem'and enum.EnumType==eType then return enum end
	
	--Check if it's a valid EnumItem's name.
	local vEName=pcall(function()return eType[enum]end)
	
	--Either returns the EnumItem associated or errors out.
	if vEName then return eType[enum]
	elseif enum then
		--error('Internal: \'enum\' is not valid.',2)
		return
	end
end

--Generates a TweenInfo from the provided options.
local function getTweenInfo(options)
	
	--Returns the default TweenInfo if it's nil.
	if not options then return TweenInfo.new()end
	
	--TweenInfo
	local ti=options.TweenInfo
	
	--Positive Float or 0
	local t=options.Time
	
	--EasingStyle
	local es=options.EasingStyle
	
	--EasingDirection
	local ed=options.EasingDirection
	
	--Positive Integer or 0
	local rp=options.RepeatCount
	
	--Boolean
	local rv=options.Reverses
	
	--Positive Float
	local dl=options.DelayTime
	
	--No need to process if you've already got it.
	if typeof(ti)=='TweenInfo' then return ti end
	
	--Checks the EasingStyle.
	if not es then es=Enum.EasingStyle.Linear else es=GetEnumValue('EasingStyle',es)end
	if not es then error('\'EasingStyle\' is not a valid value.',3)return end
	
	--Checks the EasingDirection.
	if not ed then ed=Enum.EasingDirection.InOut else ed=GetEnumValue('EasingDirection',ed)end
	if not ed then error('\'EasingDirection\' is not a valid value.',3)return end
	
	--Check the time.
	if not t then t=1 elseif typeof(t)~='number'or t<0 then
		error('\'Time\' is not a positive number; time only goes backwards when the music goes backwards too.',3)
	end
	
	--Checks out the other values (RepeatCount, reverses, delay).
	if not rp then rp=0 elseif type(rp)~='number'or rp%1>0 or rp<0 then error('\'RepeatCount\' is not a valid integer.',3)return end
	if not rv then rv=false elseif type(rv)~='boolean'then error('\'Reverses\' is not a valid boolean value.',3)return end
	if not dl then dl=0 elseif type(dl)~='number'or dl<0 then error('\'DelayTime\' is not a valid number.',3)return end
	
	--Returns the generated TweenInfo.
	return TweenInfo.new(t,es,ed,rp,rv,dl)
end

--Converts the TweenInfo to a table, so that it may be reprocessed in GetTweenInfo.
local function extractTweenInfo(ti)
	return{
		Time=ti.Time,
		EasingStyle=ti.EasingStyle,
		EasingDirection=ti.EasingDirection,
		RepeatCount=ti.RepeatCount,
		Reverses=ti.Reverses,
		DelayTime=ti.DelayTime,
	}
end

local function doTween(part,tI,props)
	if not props then props=tI tI=TweenInfo.new() end
	local t=game.TweenService:Create(part,tI,props)
	t:Play()return t
end

return{
	GetEnumValue=GetEnumValue,
	GetTweenInfo=getTweenInfo,
	ExtractTweenInfo=extractTweenInfo,
	DoTween=doTween,
