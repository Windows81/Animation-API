					<ProtectedString name="Source"><![CDATA[--Initialises the mainiest variables.
local evt=script.Event.Value
local sc=evt.Parent
local util=require(sc.Util)
local lp=game.Players.LocalPlayer

--It's all the developers' fault this has to be.
local humanoidSt8s={}
for i,st in pairs(Enum.HumanoidStateType:GetEnumItems()) do
	if st.Name~='None' then
		humanoidSt8s[#humanoidSt8s+1]=st
	end
end

--Begin exploit support region. --{B}--

--The intro to the client's dabble.
function connect(name,...)
	if name=='SPMS' then
		local char,state=...
		for i,e in pairs(humanoidSt8s) do
			--The 'not state' means that making it nil will enable all states.
			char.Humanoid:SetStateEnabled(e,not state or e==state)
		end
		char.Humanoid:ChangeState(state)
		
	--Tweens the CurrentCamera locally, since it doesn't replicate from the server.
	elseif name=='TCam' then
		local tiT,pr,tv=...
		local ti=util.GetTweenInfo(tiT)
		local cc=game.Workspace.CurrentCamera
		
		--Ensures the camera tween works well and all.
		if cc.CameraType~=Enum.CameraType.Scriptable then
			cc.CameraType='Scriptable'warn'CameraType set to \'Scriptable\''
		end
		
		--Exxxecute the plan.
		util.DoTween(cc,ti,{[pr]=tv})
		
	--MoveCamera is here.
	elseif name=='MC' then
		local cc=game.Workspace.CurrentCamera
		
		--Ensures the camera tween works well and all.
		if cc.CameraType~=Enum.CameraType.Scriptable then
			cc.CameraType='Scriptable'warn'CameraType set to \'Scriptable\''
		end
		
		--Exxxecute.
		local cf,fo=...
		cf=CFrame.new(cf.p,fo.p)
		cc.CFrame,cc.Focus=cf,fo
		
	--Toggles the graphical user interface.
	elseif name=='TGui' then
		require(sc.ToggleGui)(...)
		
	--Toggles the character.
	elseif name=='TChar' then
		local tc=require(sc.ToggleChar)
		local char=lp.Character
		local hide,selph,others=...
		
		--If they're gonna hide, then so are newcomers.
		if others then tc.setDef(hide) end
		
		--Iterating over all players.
		for i,pl in pairs(game.Players:children()) do
			
			--Other players do this.
			if pl~=lp and others then
				tc.setChar(pl,hide)
				
			--An'you player do this.
			elseif pl==lp and selph then
				tc.setChar(pl,hide)
			end
		end
	end
end

--{F}-- Finish exploit support region.

--Connects the events over.
if evt:isA'BindableEvent' then evt.Event:Connect(connect)
else evt.OnClientEvent:Connect(connect) end]]></ProtectedString>
